public abstract class Vehicle{
    protected String name;
    protected double speed;
 
    public Vehicle(String name, double speed)
    {
        this.name = name;
        this.speed = speed;
    }

    //abstrakcyjna metoda która musi być zaimplementowana przez klasy dziedziczone
    
    public abstract void move();

    public String getName();{
        return name;
    }

    public double getSpeed();{
        return speed;
    }
}

//interfejs transportu londowego
public interface LandTransport(){
    void drive();
}

//interfejs transportu powietrznego
public interface AirTransport(){
    void fly();
}

//interfejs transportu wodnego
public interface WaterTransport(){
    void sail();
}